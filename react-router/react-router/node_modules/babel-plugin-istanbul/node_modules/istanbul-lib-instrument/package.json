    "babel-cli": "^6.3.17",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.16.3",
    "chai": "^3.0.0",
    "clone": "^2.0.0",
    "coveralls": "^2.11.4",
    "cross-env": "^2.0.1",
    "documentation": "^4.0.0-beta9",
    "js-yaml": "^3.3.1",
    "jshint": "^2.8.0",
    "mocha": "^3.1.2",
    "nopt": "^3.0.6",
    "nyc": "^8.3.1",
    "standard-version": "^3.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
